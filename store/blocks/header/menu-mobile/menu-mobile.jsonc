          "blockClass": "menu-item-categorias-mobile",
          "type": "custom",
          "iconId": null,
          "highlight": false,
          "itemProps": {
              "type": "internal",
              "href": "/Candiles/d",
              "noFollow": true,
              "tagTitle": "Ver toda la categoría",
              "text": "Ver toda la categoría"
          }
      }
    },
    "menu#submenu-candiles": {
        "children": [
          "menu-item#menu-ver-categoria-candiles",
          "image#submenu-candiles"
        ],
        "props": {
            "categoryId": 1,
            "orientation": "vertical",
            "blockClass": "menu-categorias"
        }
    },
    "image#submenu-candiles":{
        "props": {
          //"maxWidth": "10%",
          "src": "/arquivos/menu1.png",
          "blockClass": "image-submenu"
        }
