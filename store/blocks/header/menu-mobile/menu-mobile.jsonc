{
    "vtex.menu@2.x:menu#category-drawer": {
        "children": [
            "flex-layout.row#mobile-logo",
            "menu-item#menu-signin-mobile",
            "menu-item#menu-category-mobile",
            "flex-layout.row#mobile",
            "menu-item#menu-centro-mobile",
            "menu-item#menu-terminos-mobile",
            "menu-item#menu-aviso-mobile",
            "menu-item#menu-preguntas-frecuentes-mobile"
        ],
        "props": {
            "orientation": "vertical"
        }
    },
    "menu-item#menu-category-mobile": {
        "props": {
            "blockClass": ["menu-item-department-mobile", "title-mobile"],
            "type": "custom",
            "iconId": null,
            "highlight": false,
            "itemProps": {
                "type": "internal",
                "noFollow": true,
                "tagTitle": "Categorías",
                "text": "CATEGORÍAS"
            }
        }
    },
    "flex-layout.row#mobile": {
      "props": {
        "colSizing": "auto",
        "orientation": "vertical",
        "blockClass": "mobile-subcategory"
      },  
      "children": [
        "menu-item#mobile-makeup",
        "menu-item#mobile-nails",
        "menu-item#mobile-coloration",
        "menu-item#mobile-electrical",
        "menu-item#mobile-mens",
        "menu-item#mobile-brands",
        "menu-item#mobile-newst",
        "menu-item#mobile-skinCare",
        "menu-item#mobile-offers"
      ]
    },

    /*ROW LOGO*/
    "flex-layout.row#mobile-logo": {
        "props": {
            "colSizing": "auto",
            "orientation": "vertical",
            "blockClass": "mobile-subcategory"
        },
        "children": [
          "logo#mobile-logo"
        ]
    },
    "logo#mobile-logo": {
        "props": {
          "title": "Logo",
          "href": "/",
          "url": "assets/logo.svg",
          "width": "180"
        }
    },

    "menu-item#mobile-candiles": {
        "props": {      
          "id": "menu-item-category-candiles",
          "type": "custom",
          "iconId": null,
          "highlight": false,
          "itemProps": {
            "type": "internal",
            "href": "/",
            "noFollow": false,
            "tagTitle": "Candiles",
            "text": "Candiles "
          },
          "blockClass": "categs-mobile"
        },
        "blocks": [
          "submenu.accordion#mob-candiles"
        ]
    },
    "submenu.accordion#mob-candiles": {
        "children": [
          "menu#submenu-candiles"
        ]    
    },
    "menu-item#menu-ver-categoria-candiles": {
