  "list-context.image-list#main-banner": {
    "title" : "Banner Principal",
    "children": [
      "slider-layout#banner-images"
    ],
    "props": {
      "fullWidth": true,
      "images": [
        {
          "image": "assets/banner_1_desktop.png",
          "mobileImage": "assets/banner_1_mobile.jpg"
        },
        {
          "image": "assets/banner_1_desktop.png",
          "mobileImage": "assets/banner_1_mobile.jpg"
        }
      ]
    }
  },
  "slider-layout#banner-images": {
    "title": "Configuración Slider Banner",
    "props": {
      "itemsPerPage": {
        "desktop": 1,
        "tablet": 1,
        "phone": 1
      },
      "autoplay": {
        "timeout": 5000
      },
      "infinite": true,
      "showNavigationArrows": "always",
      "fullWidth": true,
      "showPaginationDots": "always",
      "blockClass": "slider-primary"
    }
  },


  "flex-layout.row#title-discover":{
    "children": [
      "rich-text#title-discover"
    ],
    "props": {
      "blockClass": "categorias-title"
    }
  },
  "rich-text#title-discover": {
