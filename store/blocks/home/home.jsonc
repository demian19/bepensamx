  "flex-layout.row#left-the-most-new":{
    "children": [
      "rich-text#titulo-cat",
      "search-result-layout.customQuery#the-most-new"
    ],
    "props": {
      "width": "60%",
      "blockClass": "the-most-new"
    }
  },
  "rich-text#titulo-cat": {
    "title": "Titulo lo + nuevo",
    "props": {
      "textAlignment": "CENTER",
      "textPosition": "CENTER",
      "text": "Lo + nuevo",
      "blockClass": ["txt-cat"]
    }
  },
  "search-result-layout.customQuery#the-most-new": {
    "props": {
      "blockClass": "most-new",
      "querySchema": {
        "orderByField": "",
        "hideUnavailableItems": false,
        "maxItemsPerPage": 12,
        "queryField": "eyeliner",
        "mapField": "ft",
        "skusFilter": "ALL_AVAILABLE",
        "simulationBehavior": "skip",
        "__unstableProductOriginVtex": true
      }
    },
    "blocks": [
      "search-result-layout.desktop",
      "search-result-layout.mobile",
      "search-not-found-layout"
    ]
  },
  "flex-layout.row#right-the-most-new":{
    "children": [
